{
  "name": "copilot-runtime-server",
  "version": "1.0.0",
  "description": "CopilotKit Runtime Server with multiple AI model adapters",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only server.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@copilotkit/runtime": "^1.9.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "optionalDependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
    "@google/generative-ai": "^0.17.0",
    "@langchain/openai": "^0.3.0",
    "groq-sdk": "^0.5.0",
    "openai": "^4.47.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "copilot",
    "ai",
    "runtime",
    "typescript"
  ],
  "author": "Dave Poon",
  "license": "MIT"
}
